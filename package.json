{
	"name": "anytype",
	"version": "1.0.0",
	"description": "Anytype",
	"main": "electron.js",
	"scripts": {
		"lint": "tslint -c tslint.json -p tsconfig.json",
		"update:addon": "./update.sh",
		"start:watch": "webpack-dev-server --mode=development --env.NODE_ENV=development --history-api-fallback --cache",
		"start:watch-prod": "webpack-dev-server --mode=production --env.NODE_ENV=production --history-api-fallback --cache",
		"start:electron": "./node_modules/.bin/electron .",
		"start:electron-wait-webpack": "wait-for-localhost 8080 && ./node_modules/.bin/electron .",
		"start:dev": "npm-run-all --parallel start:watch start:electron-wait-webpack",
		"start:prod": "npm-run-all --parallel start:watch-prod start:electron-wait-webpack",
		"build": "webpack --mode=development --env.NODE_ENV=development",
		"build:prod": "webpack --mode=production --env.NODE_ENV=production -p --config webpack.config.js",
		"dist:mac": "webpack --mode=production --env.NODE_ENV=production -p && DATE=`date '+%Y-%m-%d_%H_%M'` GIT_COMMIT=`git rev-parse --short HEAD` electron-builder --macos",
		"dist:win": "webpack --mode=production --env.NODE_ENV=production -p && DATE=`date '+%Y-%m-%d_%H_%M'` GIT_COMMIT=`git rev-parse --short HEAD` electron-builder --windows",
		"dist:linux": "webpack --mode=production --env.NODE_ENV=production -p && DATE=`date '+%Y-%m-%d_%H_%M'` GIT_COMMIT=`git rev-parse --short HEAD` electron-builder --linux",
		"publish:mac": "webpack --mode=production --env.NODE_ENV=production -p && DATE=`date '+%Y-%m-%d_%H_%M'` GIT_COMMIT=`git rev-parse --short HEAD` BUILD_NUMBER=`git rev-parse --short HEAD` electron-builder --macos --publish  always",
		"publish:linux": "webpack --mode=production --env.NODE_ENV=production -p && DATE=`date '+%Y-%m-%d_%H_%M'` GIT_COMMIT=`git rev-parse --short HEAD` BUILD_NUMBER=`git rev-parse --short HEAD` electron-builder --linux --publish  always",
		"publish:win": "webpack --mode=production --env.NODE_ENV=production -p && DATE=`date '+%Y-%m-%d_%H_%M'` GIT_COMMIT=`git rev-parse --short HEAD` BUILD_NUMBER=`git rev-parse --short HEAD` electron-builder --windows --publish  always"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/anytypeio/js-anytype.git"
	},
	"author": "",
	"license": "",
	"bugs": {
		"url": "https://github.com/anytypeio/js-anytype/issues"
	},
	"homepage": "https://github.com/anytypeio/js-anytype#readme",
	"devDependencies": {
		"@types/history": "^4.7.3",
		"@types/jquery": "^3.3.31",
		"@types/react": "^16.9.16",
		"@types/react-dom": "^16.9.4",
		"@types/react-router-dom": "^4.3.5",
		"electron": "^6.1.5",
		"electron-builder": "^21.2.0",
		"file-loader": "^4.3.0",
		"npm-run-all": "^4.1.5",
		"source-map-loader": "^0.2.4",
		"ts-loader": "^6.2.1",
		"tslint": "^5.20.1",
		"tslint-react": "^4.1.0",
		"typescript": "^3.7.3",
		"wait-for-localhost-cli": "^1.1.0",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10",
		"webpack-dev-server": "^3.9.0"
	},
	"dependencies": {
		"@babel/core": "^7.7.5",
		"@types/i18n": "^0.8.6",
		"@types/intercom-web": "^2.8.7",
		"@types/is": "0.0.21",
		"@types/lodash": "^4.14.149",
		"@types/long": "^4.0.0",
		"@types/mobx-devtools-mst": "^0.9.7",
		"@types/node": "^12.12.16",
		"@types/react-beautiful-dnd": "^11.0.3",
		"@types/react-dnd": "^3.0.2",
		"@types/react-dnd-html5-backend": "^3.0.2",
		"array-move": "^2.2.1",
		"babel-loader": "^8.0.6",
		"babel-preset-env": "^1.7.0",
		"bindings": "^1.5.0",
		"blueimp-load-image": "^2.24.0",
		"css-loader": "^3.3.0",
		"electron-dl": "^1.14.0",
		"electron-log": "^3.0.9",
		"electron-updater": "^4.2.0",
		"electron-util": "^0.12.3",
		"emoji-mart": "^2.11.1",
		"file-type": "^12.4.0",
		"fs": "0.0.1-security",
		"highlight.js": "^9.16.2",
		"history": "^4.10.1",
		"install": "^0.13.0",
		"is": "^3.3.0",
		"jquery": "^3.4.1",
		"json-loader": "^0.5.7",
		"lodash": "^4.17.15",
		"lowlight": "^1.13.0",
		"mem": "^6.0.1",
		"mobx": "^5.15.0",
		"mobx-decorators": "^6.0.1",
		"mobx-devtools-mst": "^0.9.21",
		"mobx-react": "^6.1.4",
		"mobx-state-tree": "^3.15.0",
		"node-sass": "^4.13.0",
		"npm": "^6.13.3",
		"object-assign": "^4.1.1",
		"protobufjs": "^6.8.8",
		"raf": "^3.4.1",
		"react": "^16.12.0",
		"react-dom": "^16.12.0",
		"react-hot-loader": "^4.12.18",
		"react-router-dom": "^5.1.2",
		"react-sortable-hoc": "^1.10.1",
		"read-chunk": "^3.2.0",
		"readline": "^1.3.0",
		"rehype": "^9.0.1",
		"sass-loader": "^8.0.0",
		"selection-ranges": "^3.0.3",
		"sprintf-kit": "^2.0.0",
		"style-loader": "^1.0.1",
		"url-loader": "^2.3.0"
	},
	"build": {
		"appId": "com.anytype.anytype",
		"compression": "normal",
		"productName": "Anytype",
		"protocols": [
			{
				"name": "Anytype",
				"role": "Editor",
				"schemes": [
					"anytype"
				]
			}
		],
		"asarUnpack": [],
		"extraResources": [],
		"files": [
			"electron.js",
			"!dist/*",
			"dist/index.html",
			"dist/main.js",
			"build"
		],
		"mac": {
			"icon": "electron/icon.icns",
			"type": "distribution",
			"target": [
				"dmg",
				"zip"
			],
			"category": "public.app-category.productivity",
			"artifactName": "${productName}-${version}-${env.GIT_COMMIT}-${env.DATE}.${ext}",
			"publish": [
				{
					"provider": "spaces",
					"name": "anytype",
					"region": "ams3"
				}
			]
		},
		"win": {
			"target": "nsis",
			"icon": "electron/icon.ico"
		},
		"linux": {
			"icon": "electron/icon.png",
			"target": [
				"snap",
				"AppImage"
			],
			"description": "Anytype"
		},
		"publish": [
			{
				"provider": "spaces",
				"name": "anytype",
				"region": "ams3"
			}
		]
	}
}
